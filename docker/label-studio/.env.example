# Label Studio Configuration
LABEL_STUDIO_HOST=http://localhost:8080
LABEL_STUDIO_USERNAME=admin@example.com
LABEL_STUDIO_PASSWORD=admin

# PostgreSQL Configuration
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=labelstudio

# Redis Configuration
REDIS_LOCATION=redis://redis:6379/1

# Performance Settings
LABEL_STUDIO_WORKERS=4
LABEL_STUDIO_THREADS_PER_WORKER=8
LOG_LEVEL=INFO
LABEL_STUDIO_DISABLE_SIGNUP_WITHOUT_LINK=true

# OCR ML Backend Configuration
MARKER_MODEL_PATH=/models
CUDA_VISIBLE_DEVICES=0
OCR_BATCH_SIZE=8
OCR_CONFIDENCE_THRESHOLD=0.5

# Create a personal access token and add it here
# See https://labelstud.io/guide/api#Authenticate-with-a-personal-access-token
LABEL_STUDIO_API_KEY=your_api_key_here